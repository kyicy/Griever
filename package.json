{
  "name": "griever",
  "version": "0.0.2",
  "description": "I prefer not to",
  "main": "app/index.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "npm run electron-rebuild",
    "dist": "electron-packager . 'Griever' --out='dist' --overwrite",
    "deb64": "electron-installer-debian --arch amd64 --config config.json",
    "test": "npm test",
    "dist-mac": "electron-packager . 'Griever' --platform='darwin'  --out='dist' --overwrite",
    "dist-win": "electron-packager . 'Griever' --platform='win32' --arch='x64'  --out='dist' --overwrite",
    "electron-rebuild": "electron-rebuild"
  },
  "author": "kyicy",
  "license": "MIT",
  "devDependencies": {
    "electron": "^1.8.3",
    "electron-installer-debian": "^0.8.1",
    "electron-packager": "^11.1.0",
    "electron-rebuild": "^1.7.3"
  },
  "dependencies": {
    "datauri": "^1.1.0",
    "jquery-slim": "^3.0.0",
    "lodash": "^4.17.5",
    "netease-cloud-music-anonymous-api": "github:kyicy/netease-cloud-music-anonymous-api",
    "request": "^2.85.0",
    "sequelize": "^4.37.0",
    "sqlite3": "^3.1.13",
    "umzug": "^2.1.0",
    "vanilla-lazyload": "^10.4.2"
  }
